{
  "name": "ngx-card-deck-seed",
  "author": "mattmutt",
  "version": "1.0.0",
  "description": "demo Angular app for integrating the ngx-card-deck component",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattmutt/ngx-card-deck-seed.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-aot --configuration=local",
    "start:prod": "ng serve --prod --aot --configuration=production",
    "build": "npm run build:local",
    "build:local": "ng build --configuration=local --source-map=true",
    "build:stage": "ng build --prod --base-href=/test/apps/ngx-card-deck-seed/ --configuration=stage --aot --build-optimizer=true --common-chunk=false --source-map=false --output-hashing=none",
    "build:prod": "ng build --prod --base-href=/ngx-card-deck-seed/ --configuration=production --aot --build-optimizer=true --common-chunk=false --source-map=false --output-hashing=none",
    "build:prod:local": "ng build --prod --base-href=/ngx-card-deck-seed/ --configuration=production --aot --preserve-symlinks --build-optimizer=true --common-chunk=false --source-map=false --output-hashing=none",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test --source-map=false"
  },
  "dependencies": {
    "@angular/animations": "7.2.14",
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/forms": "7.2.14",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@angular/router": "7.2.14",
    "@clr/angular": "1.1.3",
    "@clr/icons": "1.1.3",
    "@clr/ui": "1.1.3",
    "ngx-card-deck": "0.0.0-beta.4",
    "@webcomponents/custom-elements": "^1.2.4",
    "core-js": "^2.6.5",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.14.0-beta.5",
    "@angular/cli": "7.2.4",
    "@angular/compiler-cli": "7.2.14",
    "@angular/language-service": "7.2.14",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-sass": "4.11.0",
    "protractor": "~5.4.2",
    "rimraf": "^2.6.3",
    "rxjs-tslint-rules": "^4.23.1",
    "ts-node": "~8.0.3",
    "tsdocs": "^1.0.2",
    "tsickle": "0.34.0",
    "tslint": "~5.13.1",
    "typescript": "3.2"
  }
}
